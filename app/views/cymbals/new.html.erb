Adding cymbals to <b><%= @bag.name %></b>

<br><br>
<%= render 'cymbals/form' %>
<br>

<% if @cymbal.bags.empty? %>
    (Bag is empty)
<% else %>
    <%= @cymbal.bags do |c| %>
        <ul>
            <li><a href="<%= cymbal_path %>"><%= c.diameter %><%= c.brand %><%= c.line %><%= c.cym_model %><%= c.cym_type %></a></li>
        </ul>
    <% end %>
<% end %>